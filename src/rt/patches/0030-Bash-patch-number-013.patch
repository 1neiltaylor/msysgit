Subject: [PATCH 30/35] Bash patch number 013

---
 msys/packages/bash/3.1/lib/readline/readline.c | 1 +
 msys/packages/bash/3.1/patchlevel.h            | 2 +-
 2 files changed, 2 insertions(+), 1 deletion(-)

diff --git a/msys/packages/bash/3.1/lib/readline/readline.c b/msys/packages/bash/3.1/lib/readline/readline.c
index 5e9767a..4cbd0d6 100644
--- a/msys/packages/bash/3.1/lib/readline/readline.c
+++ b/msys/packages/bash/3.1/lib/readline/readline.c
@@ -282,6 +282,7 @@ rl_set_prompt (prompt)
 {
   FREE (rl_prompt);
   rl_prompt = prompt ? savestring (prompt) : (char *)NULL;
+  rl_display_prompt = rl_prompt ? rl_prompt : "";
 
   rl_visible_prompt_length = rl_expand_prompt (rl_prompt);
   return 0;
diff --git a/msys/packages/bash/3.1/patchlevel.h b/msys/packages/bash/3.1/patchlevel.h
index 4bd071e..fbf719b 100644
--- a/msys/packages/bash/3.1/patchlevel.h
+++ b/msys/packages/bash/3.1/patchlevel.h
@@ -25,6 +25,6 @@
    regexp `^#define[ 	]*PATCHLEVEL', since that's what support/mkversion.sh
    looks for to find the patch level (for the sccs version string). */
 
-#define PATCHLEVEL 12
+#define PATCHLEVEL 13
 
 #endif /* _PATCHLEVEL_H_ */
